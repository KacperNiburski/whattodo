	 }
	 indexes1.push(num1)
	$("#"+i+"text").text(bank1[num1][0])
	$("#"+i+"img").prepend('<img class="theImg" src='+ bank1[num1][1]+ '/>')
	}

	var bank=[["Happy", "/assets/dark/appbar.smiley.happy.png"],
