	["Be Merry", "assets/dark/appbar.snowflake.png"],
 	["Meet New People", "assets/dark/appbar.people.multiple.png"],
	["Laugh", "assets/dark/appbar.smiley.kiki.png"],
	["Touristy", "/assets/dark/appbar.city.sanfrancisco.png"],
