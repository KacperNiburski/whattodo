	var pricecount=0
	for (var i=18; i>=13; i--) {

		$("#"+i+"img").prepend('<img class="theImg" src='+ bank3[pricecount]+ '/>' )
		pricecount++;
	}


	var bank2=["Lining up meter sticks...", "Asking robots politely...", "Using cheat codes...", "Consulting magic 8-ball...", "Stacking monkeys...", "Loading catapult...", "Thinking harder than ever...", "Asking Oracle of Delphi...", "Synergizing a lot..."];
	$(".blackness").text(bank2[Math.ceil(Math.random()*bank2.length-1)])
}

$("#try").on("click", function(){
	for (var i=1; i<=12; i++) {
		$("#"+i+"img").children()[0].remove()
	}
	start1()
})


</script>


