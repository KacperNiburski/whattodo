	["Nerdy", "/assets/dark/appbar.smiley.glasses.png"],
	["Normal", "/assets/dark/appbar.people.png"],
	["Fancy", "/assets/dark/appbar.clothes.tie.png"]];
	var indexes=[]
	for (var i=6; i>=0; i--) {
		var num=Math.ceil(Math.random()*(bank.length)-1)
		while (isInArray(num,indexes)) {
		num=Math.ceil(Math.random()*(bank.length)-1)
	 }
	 indexes.push(num)
	$("#"+i+"text").text(bank[num][0])
	$("#"+i+"img").prepend('<img class="theImg" src='+ bank[num][1]+ '/>')
